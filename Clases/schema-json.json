{
    "$schema":  "https://json-schema.org/draft/2020-12/schema",
    "title": "grafo",
    "type": "object",
    "properties": {
        "nodos": {
            "type": "object",
            "properties": {
                "nombre": {"type": "string"},
                "persona": {"type": "number"},
                "latitud": {"type": "number"},
                "longitud": {"type": "number"}
            },
            "required": ["nombre", "persona","latitud", "longitud", "conexiones"]
        },
        "conexiones": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "nodo_origen": {"type": "object"},
                    "nodo_destino": {"type": "object"},
                    "distancia": {
                        "type": "array",
                        "items": {"type": "number"}
                    },
                    "medios_transporte": {
                        "type": "array",
                        "items": {"type": "number"}
                    }
                },
                "required": ["nodo_origen","nodo_destino", "medios_transporte"]
            }
        }
    },
    "required": ["nodos","conexiones"]
}