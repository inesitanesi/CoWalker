{
    "$schema":  "https://json-schema.org/draft/2020-12/schema",
    "title": "grafo",
    "type": "object",
    "properties": {
        "nodos": {
            "type": "array",
            "items": {
              	"type": "object",
                "properties": {
                    "nombre": {"type": "string"},
                    "persona": {"type": "number"},
                    "latitud": {"type": "number"},
                    "longitud": {"type": "number"}
                },
            	"required": ["nombre", "persona","latitud", "longitud"]
            }
        },
        "conexiones": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "nodo_origen": {"type": "string"},
                    "nodo_destino": {"type": "string"},
                    "distancia": {
                        "type": "array",
                        "items": {"type": "number"}
                    },
                    "medios_transporte": {
                        "type": "array",
                        "items": {"type": "number"}
                    }
                },
                "required": ["nodo_origen","nodo_destino", "medios_transporte"]
            }
        }
    },
    "required": ["nodos","conexiones"]
}